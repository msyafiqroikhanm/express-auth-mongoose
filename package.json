{
  "name": "api.ktp-checker",
  "version": "1.0.0",
  "description": "API's KTP Checker",
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "test": "jest --detectOpenHandles --forceExit",
    "start:dev": "nodemon index.js",
    "start:prod": "node index.js",
    "format": "prettier --write '**/*.js'",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:staged": "lint-staged",
    "package:upgrade": "ncu -u",
    "package:check": "ncu -i",
    "seed": "node seeders/seed.js",
    "seed:rollback": "node seeders/undoSeed.js",
    "deadcode": "depcheck",
    "spell": "cspell lint --config cspell.json '**/*.ts' --color --gitignore --no-must-find-files --no-summary --no-progress || true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "ioredis": "^5.4.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.3.0",
    "mongoose": "^8.9.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.37.5",
    "slugify": "^1.6.6",
    "supertest": "^7.0.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write '**/*.js'",
      "eslint",
      "stop-only --file"
    ],
    "*.{json,yaml}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.22",
    "node": ">= 20.11.0"
  },
  "depcheck": {
    "ignores": [
      "@commitlint/config-conventional",
      "@commitlint/cli"
    ]
  }
}
